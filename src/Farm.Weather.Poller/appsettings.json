{
  "CommandBus": {
    "WeatherDataQueue": "farm-weather-data-commands"
  },
  "Scheduler": {
    "QueueName": "farm-masstransit-quartz-scheduler",
    "UseInMemoryScheduler": true,
    "CollectWeatherData": {
      "JobId": "CollectWeatherData",
      "CronExpression": "0 0 0/1 ? * * *"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}

